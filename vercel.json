{
    "version": 2,
    "builds": [
      {
        "src": "index.js",
        "use": "@vercel/node"
      },
      {
        "src": "index.html",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/api/image",
        "dest": "/index.js"
      },
      {
        "src": "/",
        "dest": "/index.html"
      }
    ]
}